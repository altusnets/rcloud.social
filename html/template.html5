<!DOCTYPE html>
<html$if(lang)$ lang="$lang$"$endif$$if(dir)$ dir="$dir$"$endif$>
<head>

$for(header-includes)$
$header-includes$
$endfor$

$for(css)$
  <link rel="stylesheet" href="$css$" type="text/css" />
$endfor$
  

</head>
<body>
<div id="wrapper">


$for(include-before)$
$include-before$
$endfor$

<div id="inner-wrapper" >
  
  <div class="container">
  $body$

<div>
$for(g)$
  $if(g.section)$
  </div><div class="row"><h2 class="gallery-header">$g.section$</h2>
  $else$
  <div class="col-sm-3">
    <a href="$g.link$"><img class="img-responsive img-rounded" src="$g.image$" alt=""></a>
    <h3 class="image-subtitles">$g.caption$</h3>
    <p class="gallery-desc">$g.desc$</p>
  </div>
  $endif$
$endfor$
</div>

  </div> <!-- container -->


$for(include-after)$
$include-after$
$endfor$

</div> <!-- inner-wrapper -->

</div> <!-- wrapper -->

</body>
</html>
